+		LowWord = LowWord | (operand << 20);        //[31:20]
+
+		HighWord = HighWord | (operand >> 12);      //[32]
+		opcode_fixed = 0x8C;                         //
+		HighWord = HighWord | (opcode_fixed << 1);  //[40:33]

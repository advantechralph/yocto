-	/* SW1A/1B standby voltage set to 0.975V */
-	reg = 0xb;
-	pmic_reg_write(p, PFUZE3000_SW1ASTBY, reg);
-	pmic_reg_write(p, PFUZE3000_SW1BSTBY, reg);

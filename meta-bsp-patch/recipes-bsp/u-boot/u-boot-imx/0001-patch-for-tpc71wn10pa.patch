+		| ipu_channel_params.bpp<<11 | ipu_channel_params.sdry<<10 | ipu_channel_params.sdrx<<9 | ipu_channel_params.sdy<<2 \
+		| ipu_channel_params.sce<<1 | ipu_channel_params.scc;
